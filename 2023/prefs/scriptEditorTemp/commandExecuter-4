window -width 150;
columnLayout -adjustableColumn true;
    button -label "get Next File" -command "AA()";
    button -label "run" -command "BB()";
showWindow;
proc AA(){
        $sw=getNextFile();
        if($sw) print ("ready\n");
        else print ("At the latest file \n");
}
proc BB(){
        quickSwitchOffPlugin();
        cleanUnkownNode();
        cleanUnkownPlugin();
        file -force -save -type "mayaBinary";
        print ("done\n");
}
