proc deleteConstraint(){

    string $cons[]=`listConnections -t "constraint"`;
    delete $cons;
    }

parentConstraint "*:FKNeck_M" "*:FKNeck_M_Hook";
parentConstraint "*:CloakRoot" "*:Cloak_Anchor";

select -r  "*:WeaponR_Anchor";
deleteConstraint();  


parentConstraint "IKArm_R_helper" "*:IKArm_R";
parentConstraint "IKArm_L_helper" "*:IKArm_L";

